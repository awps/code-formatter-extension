{
  "name": "__MSG_extName__",
  "version": "1.7.0",
  "manifest_version": 3,
  "description": "__MSG_extDescription__",
  "homepage_url": "https://zerowp.com/code-formatter",
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png",
    "32": "icons/32.png",
    "16": "icons/16.png"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "default_title": "__MSG_extShortName__"
  },
  "background": {
    "service_worker": "background.min.js"
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "file:///*"
  ],
  "default_locale": "en"
}
